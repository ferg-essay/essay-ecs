    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'ticker'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=ticker"
                ],
                "filter": {
                    "name": "ticker",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug integration test 'builder_test'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--test=builder_test",
                    "--package=ticker"
                ],
                "filter": {
                    "name": "builder_test",
                    "kind": "test"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}